{
    "name": "nortta/laravel-adapter",
    "description": "Laravel Adapter for Nortta",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Nortta\\Laravel\\": "src/",
            "Nortta\\Laravel\\Factories\\": "database/factories",
            "Nortta\\Laravel\\Seeders\\": "database/seeders"
        }
    },
    "require": {
        "illuminate/console": "^8.0|^9.0|^10.0|^11.0",
        "illuminate/encryption": "^8.0|^9.0|^10.0|^11.0",
        "illuminate/http": "^8.0|^9.0|^10.0|^11.0",
        "illuminate/database": "^8.0|^9.0|^10.0|^11.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Nortta\\Laravel\\NorttaServiceProvider"
            ]
        }
    }
}
